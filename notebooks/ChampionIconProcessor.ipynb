    "import os\n",
    "directory = \"../tiles\"\n",
    "\n",
    "for filename in os.listdir(directory):\n",
    "  if filename.endswith(\".jpg\"):\n",
    "    if filename.split(\"_\")[1] != \"0.jpg\":\n",
    "      filePath = os.path.join(directory, filename)\n",
    "      os.remove(filePath)"
   ]
  }
